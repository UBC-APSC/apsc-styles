{
  "name": "collab-css",
  "version": "1.0.0",
  "description": "**Purpose:** Base css for all apsc collab sites. It is referenced by all apsc collab sites in production.",
  "scripts": {
    "watch": "sass --watch apsc-scss/$npm_config_project/$npm_config_project.scss:dev/css/dev.css --color",
    "compile": "sass --update apsc-scss/$npm_config_project/$npm_config_project.scss:dist/$npm_config_project.css --no-charset --no-source-map",
    "create-project": "cp -R apsc-scss/inherited-project apsc-scss/$npm_config_project && mv apsc-scss/$npm_config_project/inherited-project.scss apsc-scss/$npm_config_project/$npm_config_project.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apsc-web/collab-css.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apsc-web/collab-css/issues"
  },
  "homepage": "https://github.com/apsc-web/collab-css#readme",
  "dependencies": {},
  "devDependencies": {
    "sass": "^1.26.5"
  }
}
